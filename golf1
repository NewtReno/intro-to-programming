#Reads each player's name and golf score as input

#Save to golf.txt

result = open('golf.txt', 'w')

while True:

    name = input('Player Name(or q to quit):')

    if name == 'q':

        break

    score = input('Player Score:')



#write to file golf.txt

    result.write(name + '\n')



    result.write(str(score) + '\n')



result.close() 
